{
  "name": "Google Reader: Open in background tab",
  "version": "0.1.2",
  "description": "Opens new tabs in background when you press 'V' in Google reader.",
  //"browser_action": {
    //"default_icon": "icon.png"
  //},
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": [
        "http://*.google.com/reader/*",
        "https://*.google.com/reader/*"
      ],
      "js": ["openinbg.js"]
    }
  ],
  "permissions": [
    "tabs",
    "http://*.google.com/reader/*",
    "https://*.google.com/reader/*"
  ]
}
